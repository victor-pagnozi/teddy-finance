{
  "name": "teddy",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:server": "turbo run dev --filter=@teddy/server",
    "dev:website": "turbo run dev --filter=@teddy/website",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rimraf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.1.3",
    "typescript": "^5.6.3",
    "rimraf": "^6.0.1"
  }
}
